# Server Configuration
PORT=3000
NODE_ENV=development

# ============================================
# GATEWAY CONFIGURATION
# ============================================

# API Authentication (comma-separated API keys)
# Leave empty to disable authentication
API_KEYS=your-api-key-1,your-api-key-2

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=100

# Retry Configuration
RETRY_MAX_ATTEMPTS=3
RETRY_DELAY_MS=1000

# ============================================
# FREE LLM PROVIDERS API KEYS
# ============================================

# OpenRouter - https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Google AI Studio (Gemini) - https://aistudio.google.com/apikey
GOOGLE_AI_API_KEY=

# NVIDIA NIM - https://build.nvidia.com
NVIDIA_NIM_API_KEY=

# Mistral (La Plateforme) - https://console.mistral.ai/api-keys
MISTRAL_API_KEY=

# Mistral Codestral - https://codestral.mistral.ai
MISTRAL_CODESTRAL_API_KEY=

# HuggingFace Inference - https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=

# Groq - https://console.groq.com/keys
GROQ_API_KEY=

# Cerebras - https://cloud.cerebras.ai
CEREBRAS_API_KEY=

# Cohere - https://dashboard.cohere.com/api-keys
COHERE_API_KEY=

# GitHub Models - https://github.com/settings/tokens
GITHUB_MODELS_TOKEN=

# Cloudflare Workers AI - https://dash.cloudflare.com
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_API_TOKEN=

# Google Cloud Vertex AI - Service Account JSON path
GOOGLE_VERTEX_PROJECT_ID=
GOOGLE_VERTEX_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=
